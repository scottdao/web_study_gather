    "newDev": "cross-env NODE_ENV=dev webpack-dev-server --mode=development  --open chrome --config webpack.config.js",
    "newBuild": "cross-env NODE_ENV=production  webpack   --mode=production --config webpack.config.js",
    "start": "http-server dist"
